{
  "name": "@test-nuxt/test-nuxt",
  "version": "0.1.0",
  "description": "test-nuxt",
  "main": "",
  "type": "module",
  "scripts": {
    "prepare": "pnpm --recursive prepare && nuxi prepare nuxt-playground",
    "dev:js": "nodemon --ext 'js,cjs,mjs,ts,json'",
    "dev:ts": "nodemon --ext 'js,cjs,mjs,ts,json' --exec ts-node --esm",
    "dev:nuxt": "nuxi dev nuxt-playground",
    "format": "prettier '**/*' --write --ignore-unknown && pnpm lint --fix",
    "lint": "eslint '**/*.{js,cjs,mjs,ts,vue}'",
    "preinstall": "npx -y only-allow pnpm"
  },
  "keywords": [],
  "author": "frohman <frohamn@alljoint.tw>",
  "private": true,
  "packageManager": "pnpm@8.5.0",
  "dependencies": {
    "nuxt": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.14",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
